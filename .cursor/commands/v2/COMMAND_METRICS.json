{
  "meta": {
    "version": "2.0",
    "created": "2025-11-03",
    "description": "Command usage metrics and performance tracking"
  },
  "tracking": {
    "enabled": true,
    "anonymized": true,
    "retention_days": 30
  },
  "metrics": {
    "session": {
      "total_commands": 0,
      "total_tokens": 0,
      "total_cost": 0.0,
      "session_start": null,
      "session_duration_minutes": 0
    },
    "commands": {},
    "performance": {
      "averages": {
        "execution_time_ms": 0,
        "tokens_per_command": 0,
        "cost_per_command": 0
      }
    }
  },
  "schema": {
    "command_entry": {
      "command": "string - command name",
      "category": "string - command category",
      "timestamp": "ISO8601 - execution time",
      "execution_time_ms": "number - time to complete",
      "tokens": {
        "input": "number - input tokens",
        "output": "number - output tokens",
        "total": "number - total tokens"
      },
      "cost": "number - estimated cost in USD",
      "status": "string - success|error|cancelled",
      "context": {
        "variables_used": "array - @variables used",
        "files_modified": "number - files changed",
        "tests_run": "number - if test command"
      },
      "hardware": {
        "workers": "number - parallel workers used",
        "cores_used": "number - CPU cores",
        "execution_tier": "string - light|medium|heavy"
      }
    }
  },
  "thresholds": {
    "warnings": {
      "tokens_per_session": 50000,
      "cost_per_session": 1.0,
      "execution_time_ms": 30000
    },
    "alerts": {
      "tokens_per_session": 100000,
      "cost_per_session": 5.0,
      "execution_time_ms": 60000
    }
  },
  "optimization_recommendations": {
    "high_token_commands": [],
    "slow_commands": [],
    "frequently_used": [],
    "rarely_used": []
  }
}

